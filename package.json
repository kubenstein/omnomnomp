{
  "name": "Omnomnomp",
  "author": "Jakub Niewczas",
  "version": "0.0.1",
  "description": "GraphQL, websockets, react fun weekend self-hackathon",
  "license": "MIT",
  "engines": {
    "node": "11.5.0",
    "yarn": "1.13.0"
  },
  "scripts": {
    "dev:frontend": "webpack-dev-server --config ./src/webpack-frontend.config.js",
    "dev:backend": "DATABASE_URL=sqlite:./db/data/dev.db DEBUG=true webpack --watch --config ./src/webpack-backend.config.js",
    "dev:db:migrate": "DATABASE_URL=sqlite:./db/data/dev.db node ./src/scripts/db-migrate.js",
    "dev:db:populateImages": "ENTRY=./src/scripts/db-populate-images OUTPUT_FILENAME='__task-populate-images.js' webpack --progress --config ./src/webpack-backend.config.js && DATABASE_URL=sqlite:./db/data/dev.db node ./.tmp/backend/__task-populate-images.js",
    "prod:frontend": "NODE_ENV=production webpack --config ./src/webpack-frontend.config.js",
    "prod:backend": "NODE_ENV=production ENTRY=./src/backend/prod-server-runner.js webpack --config ./src/webpack-backend.config.js",
    "build": "yarn run prod:frontend && yarn run prod:backend",
    "heroku-postbuild": "yarn run build",
    "server": "NODE_ENV=production node ./backend/bundled-server-runner.js"
  },
  "dependencies": {
    "express": "~4.x.x",
    "graphql": "^14.1.1",
    "node-fetch": "^2.3.0",
    "pg": "^7.8.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.1",
    "socket.io": "~2.0.x",
    "umzug": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@wdio/cli": "^5.4.14",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "~2.x.x",
    "es6-promise": "~4.x.x",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.2.1",
    "eslint-plugin-react": "^7.12.4",
    "express-graphql": "^0.7.1",
    "file-loader": "~3.x.x",
    "form-serialize": "^0.7.2",
    "fs-extra": "~3.x.x",
    "js-cookie": "^2.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "~4.x.x",
    "nodemon-webpack-plugin": "^4.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prop-types": "^15.6.0",
    "react": "~16.8.x",
    "react-dom": "~16.8.x",
    "resolve-url-loader": "^3.0.1",
    "sass-loader": "~7.x.x",
    "socket.io-client": "^2.2.0",
    "sqlite3": "^4.0.6",
    "style-loader": "~0.23.x",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "~4.29.x",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "~3.1.x"
  }
}
